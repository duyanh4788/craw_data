{
    "name": "craw_data",
    "version": "1.0.0",
    "description": "demo craw data from F1 with NodeJS, Express, MySql and TypeScript",
    "main": "src/server.ts",
    "private": true,
    "scripts": {
        "test": "jest --runInBand",
        "test:watch": "jest --runInBand --watchAll",
        "build": "tsc",
        "env-dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "env-dev-received": "ts-node-dev --respawn --transpile-only src/server_received.ts",
        "start": "node -r dotenv/config ./build/server.js",
        "watch-ts": "tsc -w",
        "eject": "nodejs-scripts eject",
        "lint": "eslint .",
        "lint:fix": "eslint --fix",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
    },
    "dependencies": {
        "@aws-sdk/lib-storage": "3.293.0",
        "@types/amqplib": "0.10.1",
        "@types/cookie-parser": "1.4.3",
        "@types/express": "4.17.13",
        "@types/express-session": "1.17.6",
        "@types/jsonwebtoken": "8.5.8",
        "@types/morgan": "1.9.4",
        "@types/winston": "2.4.4",
        "amqplib": "0.10.3",
        "authenticator": "1.1.5",
        "axios": "1.3.4",
        "cookie-parser": "1.4.6",
        "debug": "4.3.4",
        "dotenv": "^10.0.0",
        "engine.io": "6.2.0",
        "express": "4.18.1",
        "express-session": "1.17.3",
        "jsonwebtoken": "^8.5.1",
        "morgan": "1.10.0",
        "multer": "1.4.5-lts.1",
        "mysql2": "3.3.5",
        "puppeteer": "20.7.1",
        "redis": "4.6.5",
        "sequelize": "6.32.0",
        "sequelize-typescript": "2.1.5",
        "sharp": "0.31.3",
        "socket.io": "^4.6.2",
        "ts-node-dev": "2.0.0",
        "winston": "3.8.2"
    },
    "engines": {
        "node": ">=16"
    },
    "devDependencies": {
        "@types/authenticator": "1.1.1",
        "@types/sharp": "0.31.1",
        "@typescript-eslint/eslint-plugin": "5.54.1",
        "@typescript-eslint/parser": "5.54.1",
        "eslint": "8.36.0",
        "eslint-config-prettier": "8.7.0",
        "eslint-plugin-prettier": "4.2.1",
        "jest": "29.5.0",
        "prettier": "2.8.4",
        "ts-jest": "29.1.0",
        "typescript": "4.9.5"
    }
}